{
    "name": "Lisp Workshop Starter",
    "build": {
        "dockerfile": "Dockerfile",
        "args": {
            "platform": "linux/amd64",
            "network": "host"
        }
    },
    "customizations": {
        "vscode": {
            "settings": {
                "terminal.integrated.defaultProfile.linux": "bash"
            },
            "extensions": [
                "qingpeng.common-lisp"
            ]
        }
    }
}